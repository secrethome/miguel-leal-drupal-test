# Ignore directories generated by Composer.
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore Drupal's file directory.
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by IDEs.
/.idea/
/.vscode/

# Ignore .env files as they are personal.
/.env
/bitbucket-pipelines.yml

# Ignore some local only settings/services files.
/web/sites/*/settings.*.php
/web/sites/*/services.*.php

# Docksal.
.docksal

# Node.
node_modules

# Theming.
/web/themes/custom/*/assets/**/*.css
/web/themes/custom/*/assets/**/*.css.map
/web/themes/custom/*/assets/**/*.js
/web/themes/custom/*/assets/**/*.js.map
/web/themes/custom/*/assets/fonts/webfonts
/web/themes/custom/*/styleguide

# Databases.
*.sql

# Dummy OS files.
.DS_Store
/.gitattributes